    "format": "pretty-quick",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "resolutions": {
    "terser": "4.0.0",
    "sharp": "0.22.1"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.5.1",
