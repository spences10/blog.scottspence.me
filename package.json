{
  "name": "blog.scottspence.me",
  "version": "4.0.62",
  "author": "Scott Spence <spences10apps@gmail.com>(https://scottspence.me/)",
  "license": "MIT",
  "description": "Personal blog made with Gatsby v2",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
    "build": "gatsby build",
    "dev": "gatsby develop --verbose --p 8899",
    "serve": "gatsby serve --verbose",
    "buildAndServe": "npm run build && npm run serve",
    "format": "pretty-quick",
    "release": "release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/react": "^1.0.0-alpha.12",
    "@mdx-js/tag": "^0.20.3",
    "babel-plugin-styled-components": "1.10.0",
    "gatsby": "2.3.13",
    "gatsby-image": "2.0.37",
    "gatsby-mdx": "0.5.2",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-feed": "2.1.0",
    "gatsby-plugin-google-analytics": "2.0.18",
    "gatsby-plugin-manifest": "2.0.26",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-robots-txt": "1.4.0",
    "gatsby-plugin-sharp": "2.0.32",
    "gatsby-plugin-sitemap": "2.0.11",
    "gatsby-plugin-styled-components": "3.0.7",
    "gatsby-plugin-twitter": "2.0.13",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-embed-video": "1.7.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.10",
    "gatsby-remark-prismjs": "3.2.7",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.17",
    "prism-react-renderer": "0.1.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-live": "1.12.0",
    "react-tweet-embed": "1.2.2",
    "react-utterances": "0.6.4",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "release-it": "10.4.0"
  },
  "keywords": [
    "gatsby",
    "personal",
    "blog"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/blog.scottspence.me.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/blog.scottspence.me/issues"
  },
  "homepage": "https://github.com/spences10/blog.scottspence.me#readme"
