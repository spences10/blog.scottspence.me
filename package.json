{
  "name": "blog.scottspence.me",
  "version": "2.0.1",
  "author":
    "Scott Spence <spences10apps@gmail.com>(https://blog.scottspence.me/)",
  "license": "MIT",
  "description": "Personal blog made with Gatsby default starter",
  "dependencies": {
    "gatsby": "1.9.164",
    "gatsby-image": "1.0.34",
    "gatsby-link": "1.6.34",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-react-helmet": "2.0.3",
    "gatsby-plugin-sharp": "1.6.25",
    "gatsby-plugin-styled-components": "2.0.4",
    "gatsby-remark-copy-linked-files": "1.5.25",
    "gatsby-remark-images": "1.5.39",
    "gatsby-remark-prismjs": "1.2.12",
    "gatsby-source-filesystem": "1.5.14",
    "gatsby-transformer-remark": "1.7.28",
    "react-helmet": "5.2.0",
    "snyk": "1.69.1",
    "styled-components": "3.0.2"
  },
  "devDependencies": {
    "eslint": "4.16.0",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.15.0-rc.2",
    "prettier": "1.10.2",
    "pretty-quick": "1.2.2",
    "release-it": "6.1.0"
  },
  "keywords": ["gatsby", "personal", "blog"],
  "main": "n/a",
  "now": {
    "name": "scottblog"
  },
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf scottblog/ && rm -rf public/",
    "build": "gatsby build && mv public scottblog",
    "deploy:now": "npm run clean && npm run build && now scottblog/",
    "dev": "npm run clean && gatsby develop",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "deploy": "now scottblog/ --token $NOW_TOKEN",
    "alias:dev":
      "now alias blog-scottspence.now.sh --token $NOW_TOKEN",
    "alias:prod": "now alias blog.scottspence.me --token $NOW_TOKEN",
    "cleanup": "now rm scottblog --safe --yes --token $NOW_TOKEN",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/blog.scottspence.me.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/blog.scottspence.me/issues"
  },
  "homepage":
    "https://github.com/spences10/blog.scottspence.me#readme",
  "snyk": true
}
